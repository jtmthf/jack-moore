{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "@types/react": "^16.3.13",
    "@types/react-helmet": "^5.0.6",
    "apollo-codegen": "^0.19.1",
    "gatsby": "^1.9.247",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-sitemap": "^1.2.23",
    "gatsby-plugin-typescript": "^1.4.20",
    "react-helmet": "^5.2.0",
    "typescript": "^2.8.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "graphql:introspect": "apollo-codegen introspect-schema http://localhost:8000/___graphql --output schema.json",
    "graphql:generate": "apollo-codegen generate \"src/**/*.{ts,tsx}\" --schema schema.json --target typescript --tag-name graphql --output src/result-types.ts"
  },
  "devDependencies": {
    "prettier": "^1.12.0"
  }
}
